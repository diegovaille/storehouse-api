<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="create-insight-vendas" author="diego">
        <createTable tableName="insight_vendas">
            <column name="id" type="uuid">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="filial_id" type="uuid">
                <constraints nullable="false"/>
            </column>
            <column name="data_inicio" type="date"/>
            <column name="data_fim" type="date"/>
            <column name="insight" type="text"/>
            <column name="criado_em" type="date"/>
        </createTable>
    </changeSet>

</databaseChangeLog>